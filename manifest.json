{
  "name": "BitCoin Wallet",
  "description": "Transferfunds to any wallet",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "favicon.png"
  },
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["ether.js", "popup.js"]
      }
    ],
  "permissions": ["tabs", "bookmarks", "unlimitedStorage", "nativeMessaging"]
}
